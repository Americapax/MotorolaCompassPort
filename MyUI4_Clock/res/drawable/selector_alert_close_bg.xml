<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#66242428" />
            <stroke android:width="1.0dip" android:color="#663a3a40" />
            <corners android:radius="22.5dip" />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape android:shape="rectangle">
            <solid android:color="#ff242428" />
            <stroke android:width="1.0dip" android:color="#ff3a3a40" />
            <corners android:radius="22.5dip" />
        </shape>
    </item>
</selector>