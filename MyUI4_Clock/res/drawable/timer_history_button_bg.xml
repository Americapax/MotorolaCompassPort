<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/timer_history_bg_radius" />
            <stroke android:width="@dimen/timer_history_bg_line" android:color="@color/timer_record_bg" />
            <size android:height="@dimen/timer_history_btn_height" android:width="@dimen/timer_history_btn_width" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/timer_history_bg_radius" />
            <stroke android:width="@dimen/timer_history_bg_line" android:color="@color/timer_record_bg" />
            <size android:height="@dimen/timer_history_btn_height" android:width="@dimen/timer_history_btn_width" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/timer_history_bg_radius" />
            <stroke android:width="@dimen/timer_history_bg_line" android:color="@color/timer_record_bg" />
            <size android:height="@dimen/timer_history_btn_height" android:width="@dimen/timer_history_btn_width" />
        </shape>
    </item>
</selector>